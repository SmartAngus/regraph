{
  "name": "regraph",
  "version": "0.0.1-beat1",
  "description": "ReGraph",
  "main": "build/src/index.js",
  "typings": "build/src/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npx rimraf _site && mkdir _site && cross-env NODE_ENV=development bisheng start -c ./bisheng.config.js",
    "site": "npx rimraf _build && mkdir _build && cross-env NODE_ENV=production bisheng build -c ./bisheng.config.js",
    "test": "rm -rf ./build && tsc && nyc --reporter=lcov --reporter=text --reporter=json ava && rm -rf .nyc_output",
    "prepublish": "tnpm run test && tnpm run scss",
    "scss": "cp src/ReScreen/ReScreen.scss build/src/ReScreen/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nefe/ReGraph.git"
  },
